-- Query for contract_insurance_types default_value

  UPDATE contract_insurance_types
SET default_value = 0.5
WHERE id = 3; 

  UPDATE contract_insurance_types
SET default_value = 0.5
WHERE id = 5; 

  UPDATE contract_insurance_types
SET default_value = 0.75
WHERE default_value is NULL; 



